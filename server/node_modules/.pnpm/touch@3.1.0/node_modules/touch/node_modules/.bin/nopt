#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/node_modules:/workspaces/collectorx/server/node_modules:/workspaces/collectorx/node_modules:/workspaces/node_modules:/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/touch@3.1.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/nopt@1.0.10/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/node_modules:/workspaces/collectorx/server/node_modules:/workspaces/collectorx/node_modules:/workspaces/node_modules:/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/workspaces/collectorx/server/node_modules/.pnpm/touch@3.1.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
else
  exec node  "$basedir/../../../../../nopt@1.0.10/node_modules/nopt/bin/nopt.js" "$@"
fi
